<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BotaniqueExercice" table="botanique_exercice">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="true"/>
    <field name="famille" type="string" column="famille" length="255" nullable="true"/>
    <field name="nomFr" type="string" column="nom_fr" length="255" nullable="true"/>
    <field name="nomDe" type="string" column="nom_de" length="255" nullable="true"/>
    <field name="origineFr" type="string" column="origine_fr" length="255" nullable="true"/>
    <field name="origineDe" type="string" column="origine_de" length="255" nullable="true"/>
    <field name="habitatFr" type="text" column="habitat_fr" nullable="true"/>
    <field name="habitatDe" type="text" column="habitat_de" nullable="true"/>
    <field name="remarquesFr" type="text" column="remarques_fr" nullable="true"/>
    <field name="remarquesDe" type="text" column="remarques_de" nullable="true"/>
    <field name="codage" type="string" column="codage" length="4" nullable="true"/>
  </entity>
</doctrine-mapping>
